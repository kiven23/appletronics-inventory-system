ACA;AAbA,GApCA;AAmDA,EAAA,OAnDA,qBAmDA;AACA,SAAA,MAAA;AACA,QAAA,QAAA,GACA,WAAA,KAAA,MAAA,CAAA,MAAA,CAAA,MAAA,GAAA,GAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,IADA;AAEA,SAAA,QAAA,GAAA,QAAA,GAAA,MAAA;AACA;AAxDA,CAAA","sourcesContent":["<template>\n  <div class=\"q-pa-md\">\n    <div class=\"row q-col-gutter-sm\">\n      <div class=\"col\">\n        <div style=\"margin-bottom: 12px\">\n             <q-btn\n            \n            icon=\"fas fa-angle-double-left\"\n            color=\"teal\"\n            to=\"/\"\n          ></q-btn>\n         \n                <q-btn class=\"float-right\" icon=\"fas fa-download\" color=\"teal\">\n                    <export-excel\n                      :data=\"InventoryReconcData\"\n                      :fields=\"json_fields\"\n                      worksheet=\"My Worksheet\"\n                      :name=\"filename\"\n                    >\n                    </export-excel>\n                  </q-btn>\n           \n          </div>\n        <q-table\n          dense\n          flat\n          bordered\n          class=\"statement-table\"\n          title=\"Branch Inventory Reconcilation\"\n          :data=\"InventoryReconcData\"\n          :hide-header=\"mode === 'grid'\"\n          :columns=\"InventoryReconColumns\"\n          row-key=\"__index\"\n          :grid=\"mode == 'grid'\"\n          :filter=\"filter\"\n          virtual-scroll\n        ><template v-slot:top-right=\"\">\n            <q-input\n              outlined\n              dense\n              debounce=\"300\"\n              v-model=\"filter\"\n              placeholder=\"Search\"\n            ><template v-slot:append>\n                <q-icon name=\"search\" />\n              </template>\n            </q-input>\n            <q-btn\n              flat\n              round\n              dense\n              :icon=\"mode === 'grid' ? 'list' : 'grid_on'\"\n              @click=\"mode = mode === 'grid' ? 'list' : 'grid'\"\n            ><q-tooltip :disable=\"$q.platform.is.mobile\" v-close-popup>{{\n                mode === \"grid\" ? \"List\" : \"Grid\"\n              }}</q-tooltip>\n            </q-btn>\n            <div class=\"q-pa-sm q-gutter-sm\"></div>\n          </template>\n        </q-table>\n      </div>\n    </div>\n    <q-dialog\n      v-model=\"dialog\"\n      persistent\n      :maximized=\"maximizedToggle\"\n      transition-show=\"slide-up\"\n      transition-hide=\"slide-down\"\n    >\n      <q-card class=\"bg-glossy text-white\">\n        <q-bar>\n          <q-space />\n          <q-btn\n            dense\n            flat\n            icon=\"minimize\"\n            @click=\"maximizedToggle = false\"\n            :disable=\"!maximizedToggle\"\n          ><q-tooltip\n              v-if=\"maximizedToggle\"\n              content-class=\"bg-white text-primary\"\n              >Minimize</q-tooltip\n            >  </q-btn>\n          <q-btn\n            dense\n            flat\n            icon=\"crop_square\"\n            @click=\"maximizedToggle = true\"\n            :disable=\"maximizedToggle\"\n          ><q-tooltip\n              v-if=\"!maximizedToggle\"\n              content-class=\"bg-white text-primary\"\n              >Maximize</q-tooltip\n            >\n          </q-btn>\n          <q-btn dense flat icon=\"close\" v-close-popup>\n            <q-tooltip content-class=\"bg-white text-primary\">Close</q-tooltip>\n          </q-btn>\n        </q-bar>\n        <q-card-section>\n          <iframe\n            :src=\"code\"\n            height=\"900px\"\n            width=\"700px\"\n            title=\"Iframe Example\"\n          ></iframe>\n        </q-card-section>\n      </q-card>\n    </q-dialog>\n  </div>\n</template>\n\n<script>\nconst axios = require(\"axios\");\nconst token = localStorage.getItem(\"user-token\");\nconst instance = axios.create({\n  baseURL: \"https://appletronics-api.webportal.ac/api/\",\n  headers: {\n    Authorization: \"Bearer \" + token,\n  },\n});\n\nconst defaultItem = {\n  brand: \"\",\n  partname: \"\",\n  partno: \"\",\n  sap: \"\",\n  phy: \"\",\n  me: \"\",\n  unitcost: \"\",\n  totalcost: \"\",\n};\n\nconst InventoryReconColumns = [\n  {\n    name: \"brand\",\n    align: \"left\",\n    label: \"BRAND\",\n    field: \"brand\",\n    sortable: true,\n  },\n\n  {\n    name: \"partname\",\n    align: \"left\",\n    label: \"PARTS NAME\",\n    field: \"partname\",\n    sortable: true,\n  },\n  {\n    name: \"partno\",\n    align: \"left\",\n    label: \"PART NO.\",\n    field: \"partnameno\",\n    sortable: true,\n  },\n  {\n    name: \"sap\",\n    align: \"left\",\n    label: \"SAP\",\n    field: \"sap\",\n    sortable: true,\n  },\n  {\n    name: \"physical\",\n    align: \"left\",\n    label: \"PHY\",\n    field: \"phy\",\n    sortable: true,\n  },\n  {\n    name: \"me\",\n    align: \"left\",\n    label: \"M/E\",\n    field: \"me\",\n    sortable: true,\n  },\n  {\n    name: \"unitcost\",\n    align: \"left\",\n    label: \"UNIT COST\",\n    field: \"unitprice\",\n    sortable: true,\n  },\n  {\n    name: \"totalcost\",\n    align: \"left\",\n    label: \"TOTAL COST\",\n    field: \"total\",\n    sortable: true,\n  },\n];\n\nconst InventoryReconcData = [];\n\nexport default {\n  data() {\n    return {\n      json_fields: {\n        BRAND: \"brand\",\n        \"PARTS NAME\": \"partname\",\n        \"PART NO.\": \"partnameno\",\n        SAP: \"sap\",\n        PHY: \"phy\",\n        \"M/E\": \"me\",\n        \"UNIT COST\": \"unitprice\",\n        \"TOTAL COST\": \"total\",\n      },\n      filename: null,\n      phy: [],\n      dataurl: \"\",\n      dialog: false,\n      maximizedToggle: true,\n      inFs: false,\n      noti: () => {},\n      show_dialog: false,\n      editedIndex: -1,\n      editedItem: defaultItem,\n      filter: \"\",\n      mode: \"list\",\n      code: null,\n      InventoryReconColumns: InventoryReconColumns,\n      InventoryReconcData: InventoryReconcData,\n      pagination: {\n        page: 1,\n      },\n      page: 1,\n      totalRecord: 0,\n      pageCount: 1,\n    };\n  },\n  methods: {\n    render() {\n      instance\n        .post(\"/inventory/index\", {\n          branch: this.$route.params.branch,\n          asof: this.$route.params.asof,\n        })\n        .then((res) => {\n          this.InventoryReconcData = res.data;\n        });\n    },\n    exportData() {\n      alert();\n    },\n  },\n  mounted() {\n    this.render();\n    let filename =\n      \"invty-\" + this.$route.params.branch + \"-\" + this.$route.params.asof;\n    this.filename = filename + \".xls\";\n  },\n};\n</script>"],"sourceRoot":"src/components/inventory"}]}