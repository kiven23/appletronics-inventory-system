sourcesContent":["import Vue from 'vue'\nimport router from './router'\nimport axios from 'axios'\nVue.mixin({\n    methods: {\n        checkAuth() {\n            const token = localStorage.getItem('user-token');\n            axios.post('https://appletronics-api.webportal.ac/api/login/auth', {\n                ms: 'go',\n            }, {\n                headers: {\n                    Authorization: 'Bearer ' + token\n                }\n            }).then(() => {\n\n                if (this.$route.name == 'login') {\n                    router.push('/')\n                }\n            }).catch(error => {\n                localStorage.removeItem('user-token')\n                router.push('/login')\n                console.log(error)\n            })\n        },\n \n    }\n})"]}]}